<!-- Page -->
<div class="page vertical-align text-xs-center" data-animsition-in="fade-in" data-animsition-out="fade-out">>
  <div class="page-content vertical-align-middle animation-slide-top animation-duration-1">
    <div class="panel" style="width:900px">
      <div class="container">
        <div class="panel-body">

          <div class="brand">
            <%= image_tag "hot-topic-logo.png", class:"brand-img", alt:"Logo", width: 150 %>
            <p>Dropship Program</p>
            <!--<h2 class="brand-text font-size-18">Remark</h2>-->
          </div>
          <br>

          <div class="pearls row">
            <div class="pearl col-xs-3 done disabled" aria-expanded="false">
              <div class="pearl-icon"><i class="icon wb-user" aria-hidden="true"></i></div>
              <span class="pearl-title">Account Information</span>
            </div>
            <div class="pearl col-xs-3 done disabled" aria-expanded="true">
              <div class="pearl-icon"><i class="icon wb-file" aria-hidden="true"></i></div>
              <span class="pearl-title">Terms</span>
            </div>
            <div class="pearl col-xs-3 done disabled" aria-expanded="false">
              <div class="pearl-icon"><i class="icon wb-payment" aria-hidden="true"></i></div>
              <span class="pearl-title">Make Payment</span>
            </div>
            <div class="pearl col-xs-3 done current" aria-expanded="false">
              <div class="pearl-icon"><i class="icon wb-check" aria-hidden="true"></i></div>
              <span class="pearl-title">Confirmation</span>
            </div>
          </div>

          <p>
            <br>
            Thank you for your payment!<br><br>
            Your application will be reviewed within the next 1-3 weeks.<br><br>
            If you have any questions, please contact dropship-support@hottopic.com <br>
          </p>

        </div>
      </div>
    </div>
    <footer class="page-copyright page-copyright-inverse">

      <p style="color:#000;">powered by DiCentral <%= image_tag "dicentral-square-24px-blue.png", class:"img-responsive", alt:"Logo" %></p>

