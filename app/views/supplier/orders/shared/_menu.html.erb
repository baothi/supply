<div class="page-aside">
  <div class="page-aside-switch">
    <i class="icon wb-chevron-left" aria-hidden="true"></i>
    <i class="icon wb-chevron-right" aria-hidden="true"></i>
  </div>
  <div class="page-aside-inner page-aside-scroll">
    <div data-role="container">
      <div data-role="content">
        <section class="page-aside-section">
          <h5 class="page-aside-title">Orders</h5>
          <div class="list-group">
              <a class="list-group-item <%= check_if_active_css(yield(:inner_menu), Hingeto::Section::ORDERS) %>" href="/supplier/orders">
               <i class="icon wb-order" aria-hidden="true"></i>Store Orders
              </a>
            <% if current_supplier.allow_order_issue_reporting %>
              <a class="list-group-item <%= check_if_active_css(yield(:inner_menu), Hingeto::Section::REPORTED_ORDERS) %>" href="/supplier/orders/reported">
               <i class="icon fa fa-flag" aria-hidden="true"></i>Reported Orders
              </a>
            <% end %>
          </div>
        </section>
      </div>
    </div>
  </div>
  <!---page-aside-inner-->
</div>
