<div class="panel">
  <div class="panel-heading">
    <!--<h3 class="panel-title">&nbsp;</h3>-->
    <h4 class="panel-title ">
      Additional Information
    </h4>
  </div>

  <div class="panel-body">

    <div class="form-group">
      <h5>Purchase Order Number</h5>
      <div class="input-group">
        <%= text_field_tag 'purchase_order_number',
                           @order.purchase_order_number,
                           class: "form-control", disabled: true %>
      </div>
    </div>

    <div class="form-group">
      <h5>Supplier Order Number</h5>
      <div class="input-group">
        <%= text_field_tag 'supplier_shopify_name',
                           @order.supplier_shopify_order_name,
                           class: "form-control", disabled: true %>
      </div>
    </div>

    <div class="form-group">
      <h5>Sent to Supplier</h5>
      <div class="input-group">
        <%= text_field_tag 'shopify_sent_at',
                           @order.shopify_sent_at,
                           class: "form-control", disabled: true %>
      </div>
    </div>


  </div>





</div>
