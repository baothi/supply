<%
  menu = [
      {
          section: Hingeto::Section::MY_ACCOUNT,
          title: 'Frequently Asked Questions',
          link: ENV['SUPPLY_HELP_LINK'],
          secret: false,
          new_window: true
      },
      {
          section: Hingeto::Section::PASSWORD,
          title: 'Retailer Guide',
          link: ENV['GUIDE_RETAILER_URL'],
          secret: false,
          new_window: true
      }
  ]
%>

<ul class="site-menu-sub">
  <% menu.each do |menu| %>
      <%= inner_menu_html(
              inner_menu: yield(:inner_menu),
              link: menu[:link],
              section: menu[:section],
              title: menu[:title],
              secret: menu[:secret],
              new_window: menu[:new_window],
              true_user: true_spree_user
          )
      %>
  <% end %>
</ul>